{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AudioResult",
  "type": "object",
  "required": [
    "version",
    "status",
    "processing_time_sec",
    "transcript",
    "speakers",
    "stages"
  ],
  "properties": {
    "version": {"type": "string"},
    "status": {"type": "string"},
    "error": {"type": "string"},
    "error_code": {"type": "string"},
    "trace_id": {"type": "string"},
    "processing_time_sec": {"type": "number"},
    "transcript": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "start": {"type": ["number", "string", "null"]},
          "end": {"type": ["number", "string", "null"]},
          "speaker": {"type": ["string", "null"]},
          "text": {"type": ["string", "null"]}
        },
        "additionalProperties": true
      }
    },
    "speakers": {"type": "object"},
    "stages": {"type": "object"}
  },
  "additionalProperties": true
}
